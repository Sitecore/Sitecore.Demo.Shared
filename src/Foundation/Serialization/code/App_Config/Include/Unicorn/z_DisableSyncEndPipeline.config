<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/"
               xmlns:unicorn="http://www.sitecore.net/xmlconfig/unicorn/">
  
  <!--todo: remove this patch file when we update unicorn for 9.2 -->
  <sitecore unicorn:require="On">
    <pipelines>
      <unicornSyncComplete>
        <patch:delete />
      </unicornSyncComplete>

      <unicornSyncComplete />

      <unicornSyncEnd>
        <patch:delete /> 
      </unicornSyncEnd>

      <unicornSyncEnd />

    </pipelines>
  </sitecore>
</configuration>
