<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
      <settings>
        <setting name="Demo.Enabled" value="true" />
        <setting patch:instead="setting[@name='Analytics.Patterns.MinimalProfileScoreCount']" name="Analytics.Patterns.MinimalProfileScoreCount" value="1" />
      </settings>
      <pipelines>
        <initialize>
          <processor type="Sitecore.Demo.Shared.Feature.Demo.Pipelines.RegisterWebApiRoutes, Sitecore.Demo.Shared.Feature.Demo" patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeRoutes, Sitecore.Mvc']" resolve="true" />
        </initialize>
        <startAnalytics>
          <processor type="Sitecore.Demo.Shared.Feature.Demo.Pipelines.FakeTrackerData, Sitecore.Demo.Shared.Feature.Demo" method="GetFakeTrackerData" patch:before="processor[@type='Sitecore.Analytics.Pipelines.StartAnalytics.StartTracking, Sitecore.Analytics']" resolve="true" />
          <processor type="Sitecore.Demo.Shared.Feature.Demo.Pipelines.FakeTrackerData, Sitecore.Demo.Shared.Feature.Demo" method="SetFakeTrackerData" patch:after="processor[@type='Sitecore.Analytics.Pipelines.StartAnalytics.StartTracking, Sitecore.Analytics']" resolve="true" />
        </startAnalytics>
      </pipelines>
    </sitecore>
</configuration>